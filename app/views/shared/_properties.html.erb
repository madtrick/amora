<table class="table table-striped">
  <thead>
    <tr>
      <th><%= t('property') %></th>
      <th><%= t('value') %></th>
    </tr>
  </thead>
  <tbody>
<% @resource.fetched_properties.each do |name, value| %>
  <tr>
    <td><%= name %></td>
    <td><%= value %></td>
  </tr>
<% end %>
  </tbody>
</table>
